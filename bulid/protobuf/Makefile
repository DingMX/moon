  RESCOMP = windres
  TARGETDIR = ../../bin/Debug
  TARGET = $(TARGETDIR)/protobuf.dll
  OBJDIR = obj/Win32/Debug
  DEFINES += -DDEBUG -D_USRDLL
  INCLUDES += -I../../third/liblua -I../../third
  FORCE_INCLUDE +=
  ALL_CPPFLAGS += $(CPPFLAGS) -MMD -MP $(DEFINES) $(INCLUDES)
  ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) -m32 -g /TP
  ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CFLAGS) -std=c++14
  ALL_RESFLAGS += $(RESFLAGS) $(DEFINES) $(INCLUDES)
  LIBS += -llua
  LDDEPS += ../../bin/Debug/lua.lib
  ALL_LDFLAGS += $(LDFLAGS) -L/usr/lib32 -L../../bin/Debug -m32 -shared -Wl,--out-implib="../../bin/Debug/protobuf.lib"
  LINKCMD = $(CXX) -o "$@" $(OBJECTS) $(RESOURCES) $(ALL_LDFLAGS) $(LIBS)
  define PREBUILDCMDS
  endef
  define PRELINKCMDS
  endef
  define POSTBUILDCMDS
  endef
all: $(TARGETDIR) $(OBJDIR) prebuild prelink $(TARGET)
