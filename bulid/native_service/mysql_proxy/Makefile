ifeq ($(config),debug_win32)
  RESCOMP = windres
  TARGETDIR = ../../../bin/Debug
  TARGET = $(TARGETDIR)/mysql_proxy.dll
  OBJDIR = obj/Win32/Debug
  DEFINES += -DDEBUG -D_USRDLL
  INCLUDES += -I../../../native_service/mysql_proxy -I../../../third -I../../../third/liblua -I../../../xnet
  FORCE_INCLUDE +=
  ALL_CPPFLAGS += $(CPPFLAGS) -MMD -MP $(DEFINES) $(INCLUDES)
  ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) -m32 -g
  ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CFLAGS) -std=c++14
  ALL_RESFLAGS += $(RESFLAGS) $(DEFINES) $(INCLUDES)
  LIBS += -Wl,--start-group -llua -lxnet -Wl,--end-group
  LDDEPS += ../../../bin/Debug/lua.lib ../../../bin/Debug/xnet.lib
  ALL_LDFLAGS += $(LDFLAGS) -L/usr/lib32 -L../../../bin/Debug -m32 -shared -Wl,--out-implib="../../../bin/Debug/mysql_proxy.lib"
  LINKCMD = $(CXX) -o "$@" $(OBJECTS) $(RESOURCES) $(ALL_LDFLAGS) $(LIBS)
  define PREBUILDCMDS
  endef
  define PRELINKCMDS
  endef
  define POSTBUILDCMDS
  endef
all: $(TARGETDIR) $(OBJDIR) prebuild prelink $(TARGET)
	@:

endif

