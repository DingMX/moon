  DEFINES += -DNDEBUG -DASIO_STANDALONE -DASIO_HAS_STD_ARRAY -DASIO_HAS_STD_TYPE_TRAITS -DASIO_HAS_STD_SHARED_PTR -DASIO_HAS_CSTDINT -DASIO_DISABLE_SERIAL_PORT -DASIO_HAS_STD_CHRONO -DASIO_HAS_MOVE -DASIO_HAS_VARIADIC_TEMPLATES -DASIO_HAS_CONSTEXPR -DASIO_HAS_STD_SYSTEM_ERROR -DASIO_HAS_STD_ATOMIC -DASIO_HAS_STD_FUNCTION -DASIO_HAS_STD_THREAD -DASIO_HAS_STD_MUTEX_AND_CONDVAR -DASIO_HAS_STD_ADDRESSOF
